pipeline{
    agent any
    environment {
        PATH  = "$PATH:/usr/share/maven/bin"
    }
    stages{
        stage('GetCode') {
            steps{
                git 'https://github.com/adeshbagale/Sonarqube-repo.git'
            }
        } 
        stage('Build') {
            steps{
                sh 'mvn clean package'
            }
        }
        stage('Sonarqube Analysis'){
            steps {
                withSonarqubeEnv('sonarqube 8.9.2'){
                    sh "mvn sonar:sonar"
                }
            }
        }
      }

}
