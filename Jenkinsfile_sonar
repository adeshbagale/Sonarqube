pipeline{
    agent any
    environment {
        PATH  = "$PATH:/usr/share/maven/bin"
    }
    stages{ 
        stage('Build') {
            steps{
                sh 'mvn clean package'
            }
        }
        stage('Sonarqube Analysis'){
            steps {
                withSonarqubeEnv('sonarqube 8.9.2'){
                    sh "mvn sonar:sonar"
                }
            }
        }
      }

}
